<p id="notice"><%= notice %></p>

<h1>Admin Collections</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_collections.each do |admin_collection| %>
      <tr>
        <td><%= link_to 'Show', admin_collection %></td>
        <td><%= link_to 'Edit', edit_admin_collection_path(admin_collection) %></td>
        <td><%= link_to 'Destroy', admin_collection, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Admin Collection', new_admin_collection_path %>
